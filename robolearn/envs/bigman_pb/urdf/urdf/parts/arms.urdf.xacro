    <child link="RShy"/>
    <origin xyz="0.0 0.0 -0.222" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit velocity="6.283" effort="120" lower="${-164*toRad}" upper="${162.0*toRad}" />
    <dynamics friction="0" damping="3.0"/> 
</joint>

